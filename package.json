{
    "name": "dcnapp",
    "version": "1.6.0",
    "main": "main.js",
    "author": "senpai-10",
    "repository": "https://github.com/DCNAPP/DCNAPP",
    "devDependencies": {
        "electron": "^10.1.5",
        "electron-builder": "^22.9.1"
    },
    "scripts": {
        "start": "electron .",
        "build": "electron-builder",
